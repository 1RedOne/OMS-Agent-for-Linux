<source>
  type tail
  format none
  path /var/log/memcached.log
  pos_file /var/opt/microsoft/omsagent/state/out_oms_api_memcached.pos
  read_from_head true
  tag oms.api.Memcached
</source>

<filter oms.api.Memcached>
  type record_transformer
  enable_ruby
  <record>
    Computer ${OMS::Common.get_hostname}
  </record>
</filter>